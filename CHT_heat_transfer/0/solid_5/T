FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0/solid_5";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 0 0 1 0 0 0 ];

internalField   uniform 300;

boundaryField
{
    frondAndBackSolid
    {
        type            empty;
    }
    heatWall
    {
        type            fixedValue;
        value           uniform 320;
    }
    defaultFaces
    {
        type            zeroGradient;
        value           uniform 300;
    }
    solid_5_to_fluid
    {
        type            compressible::turbulentTemperatureCoupledBaffleMixed;
        value           uniform 300;
        Tnbr            T;
        kappaMethod     solidThermo;
    }
}


